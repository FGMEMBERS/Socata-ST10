<?xml version="1.0"?>
<PropertyList>

    <path>cb_30.ac</path>

    <animation>
        <type>pick</type>
        <object-name>breaker</object-name>
        <visible>true</visible>
        <action>
          <button>0</button>
          <repeatable>false</repeatable>
          <binding>
            <command>property-toggle</command>
            <property>/controls/electric/circuitbreaker/rightecubus</property>
          </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>breaker</object-name>
        <property>/controls/electric/circuitbreaker/rightecubus</property>
        <factor>0.007</factor>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    
</PropertyList>