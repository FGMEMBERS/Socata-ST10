<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <fx>
    
    <!-- ENGINE -->
    <!-- FRONT ENGINE -->
    <crank>
      <name>engstart1</name>
      <path>Sounds/engstart.wav</path>
      <property>engines/engine[0]/cranking</property>
      <position>
        <x> -1.8 </x>
        <y>  0.0 </y>
        <z>  0.0 </z>
      </position>
      <volume>
        <offset> 0.5 </offset>
      </volume>
    </crank>
    
    <crank>
      <name>crank1</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <condition>
        <property>engines/engine[0]/cranking</property>
        <not>
          <property>engines/engine[0]/running</property>
        </not>
      </condition>
      <position>
        <x> -1.8 </x>
        <y>  0.0 </y>
        <z>  0.0 </z>
      </position>
      <volume>
        <offset>0.5</offset>
      </volume>
    </crank>
    
    <crank>
      <name>cough1</name>
      <path>Sounds/coughing.wav</path>
      <property>engines/engine[0]/running</property>
      <position>
        <x> -1.8 </x>
        <y>  0.0 </y>
        <z>  0.0 </z>
      </position>
      <volume>
        <offset>0.3</offset>
      </volume>
    </crank>
    
    <engine>
      <name>engine-external-1</name>
      <mode>looped</mode>
      <path>Aircraft/Socata-ST10/Sounds/Engine_Continental-O200A.wav</path>
      <position>
        <x> -1.8 </x>
        <y>  0.0 </y>
        <z>  0.0 </z>
      </position>
      <condition>
        <property>engines/engine[0]/running</property>
        <not>
          <property>/sim/current-view/internal</property>
        </not>
      </condition>
      <volume>
        <property>engines/engine[0]/mp-osi</property>
        <factor>0.018</factor>
        <min>0.35</min>
        <max>2</max>
        <offset>0.35</offset>
      </volume>
      <pitch>
        <property>engines/engine[0]/rpm</property>
        <factor>0.0005</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0.3</offset>
      </pitch>
    </engine>
    
    <engine>
      <name>engine-internal-1</name>
      <mode>looped</mode>
      <path>Aircraft/Socata-ST10/Sounds/Engine_Continental-O200A-LPF.wav</path>
      <condition>
        <and>
          <property>engines/engine[0]/running</property>
          <property>/sim/current-view/internal</property>
        </and>  
      </condition>
      <position>
        <x>-1.8</x>
        <y> 0  </y>
        <z> 0  </z>
      </position>
      <volume>
        <property>engines/engine[0]/mp-osi</property>
        <factor>0.012</factor>
        <min>0.4</min>
        <max>1.5</max>
        <offset>0.4</offset>
      </volume>
      <pitch>
        <property>engines/engine[0]/rpm</property>
        <factor>0.0005</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0.3</offset>
      </pitch>
    </engine>
    
    <!-- FLAPS & CONTROLS -->
    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Aircraft/Socata-ST10/Sounds/Cessna150Flaps.wav</path>
      <property>surface-positions/flap-pos-norm</property>
      <volume>
        <factor>0.8</factor>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
    </flap>
    
    <rudder>
      <name>Rudder pedals noise L</name>
      <path>Aircraft/Socata-ST10/Sounds/RudderPedalNoise.wav</path>
      <condition>
        <less-than>
          <property>controls/flight/rudder</property>
          <value>-0.25</value>
        </less-than>
      </condition>
      <volume>
        <factor>0.75</factor>
      </volume>
    </rudder>
    
    <rudder>
      <name>Rudder pedals noise R</name>
      <path>Aircraft/Socata-ST10/Sounds/RudderPedalNoise.wav</path>
      <condition>
        <greater-than>
          <property>controls/flight/rudder</property>
          <value>0.25</value>
        </greater-than>
      </condition>
      <volume>
        <factor>0.75</factor>
      </volume>
    </rudder>
    
    <!-- DOOR -->
    <door>
      <name>Door Open</name>
      <path>Aircraft/Socata-ST10/Sounds/DoorOpen.wav</path>
      <condition>
        <greater-than>
          <property>sim/model/door-positions/crew/position-norm</property>
          <value>0.1</value>
        </greater-than>
      </condition>
      <volume>
        <factor>0.8</factor>
      </volume>
    </door>
    <door>
      <name>Door Close</name>
      <path>Aircraft/Socata-ST10/Sounds/DoorClosed.wav</path>
      <condition>
        <equals>
          <property>sim/model/door-positions/crew/position-norm</property>
          <value>0</value>
        </equals>
      </condition>
      <volume>
        <factor>1</factor>
      </volume>
    </door>
    
    <!-- RUMBLE & SQUEAL -->
    <rumble>
      <name>Rumble</name>
      <mode>looped</mode>
      <path>Sounds/rumble.wav</path>
      <condition>
        <or>
          <property>gear/gear[0]/wow</property>
          <property>gear/gear[1]/wow</property>
          <property>gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>velocities/groundspeed-kt</property>
        <type>log</type>
        <factor>0.35</factor>
      </volume>
    </rumble>
    
    <squeal>
      <name>Squeal</name>
      <path>Aircraft/Socata-ST10/Sounds/Squeal.wav</path>
      <condition>
        <or>
          <property>gear/gear[0]/wow</property>
          <property>gear/gear[1]/wow</property>
          <property>gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <factor>0.8</factor>
      </volume>
      <pitch>
        <property>velocities/groundspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.2</offset>
      </pitch>
      <position>
        <x> 0.8 </x>
        <y> 0   </y>
        <z>-1.28</z>
      </position>
      <reference-dist>20.0</reference-dist>
      <max-dist>50.0</max-dist>
    </squeal>
    
    <!-- CRASH & ALARMS -->
    <crash>
      <name>Intense ground contact</name>
      <mode>once</mode>
      <path>Sounds/crash.wav</path>
      <property>sim/crashed</property>
      <volume>
        <factor>1</factor>
      </volume>
      <pitch>
        <factor>0.1</factor>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>500</max-dist>
    </crash>
    
    <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>Aircraft/Socata-ST10/Sounds/Stall.wav</path>
      <condition>
        <and>
          <or>
            <property>controls/engines/engine/master-bat</property>
            <property>controls/engines/engine[0]/master-alt</property>
          </or>
          <less-than>
            <property>velocities/airspeed-kt</property>
            <value>65</value>
          </less-than>
          <not>
            <property>gear/gear[0]/wow</property>
            <property>gear/gear[1]/wow</property>
            <property>gear/gear[2]/wow</property>
          </not>
        </and>
      </condition>
      <volume>
        <factor>0.7</factor>
      </volume>
      <reference-dist>4.0</reference-dist>
      <max-dist>10.0</max-dist>
    </stall>
    
    <gear-warn>
      <name>gear warning</name>
      <mode>looped</mode>
      <path>Sounds/gear-hrn.wav</path>
      <condition>
        <or>
          <and>
            <property>gear/gear[1]/wow</property>
            <not>
              <property>controls/gear/gear-down</property>
            </not>
          </and>
          <and>
            <or>
              <less-than>
                <property>gear/gear[0]/position-norm</property>
                <value>0.95</value>
              </less-than>
              <less-than>
                <property>gear/gear[1]/position-norm</property>
                <value>0.95</value>
              </less-than>
              <less-than>
                <property>gear/gear[2]/position-norm</property>
                <value>0.95</value>
              </less-than>
            </or>
            <less-than>
              <property>controls/engines/engine[0]/throttle</property>
              <value>0.2</value>
            </less-than>
            <or>
              <less-than>
                <property>/position/altitude-agl-ft</property>
                <value>500</value>
              </less-than>
            </or>
          </and>
        </or>
      </condition>
      <position>
        <x>-2.0</x>
        <y>0.40</y>
        <z>1.40</z>
      </position>
      <volume>
        <factor>0.6</factor>
      </volume>
      <reference-dist>2.0</reference-dist>
      <max-dist>5.0</max-dist>
    </gear-warn>
    
    <gear>
      <name>gear</name>
      <mode>in-transit</mode>
      <path>Sounds/gear.wav</path>
      <property>gear/gear[0]/position-norm</property>
      <pitch>
        <internal>dt_play</internal>
        <offset>1.0</offset>
        <factor>-0.001</factor>
      </pitch>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
    </gear>
    <gear>
      <name>gear-lock</name>
      <path>Sounds/gear-lck.wav</path>
      <condition>
        <equals>
          <property>gear/gear[0]/position-norm</property>
          <value>0</value>
        </equals>
      </condition>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
    </gear>
    
    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>velocities/airspeed-kt</property>
      <volume>
        <property>position/altitude-ft</property>
        <factor>-0.000015</factor>
        <offset>0.4</offset>
        <min>0.1</min>
        <max>0.4</max>
      </volume>
      <volume>
        <property>velocities/airspeed-kt</property>
        <factor>0.0007</factor>
        <min>0.03</min>
        <max>0.12</max>
      </volume>
      <pitch>
        <property>velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>1.25</offset>
      </pitch>
    </wind>
    
    <!-- SWITCHES -->
    <!-- BATTERY -->
    <click> 
      <name>Battery ON (switch)</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonPower.wav</path>
      <condition> 
        <property>controls/electric/battery-switch</property>
      </condition>
      <volume> 
        <factor>0.7</factor>
      </volume>
    </click>
    
    <click> 
      <name>Battery ON (noise)</name>
      <path>Aircraft/Socata-ST10/Sounds/BatteryStart.wav</path>
      <condition> 
        <property>controls/electric/battery-switch</property>
      </condition>
      <volume> 
        <factor>0.2</factor>
      </volume>
    </click>
    
    <click> 
      <name>Battery OFF</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonPower.wav</path>
      <condition> 
        <not> 
          <property>controls/electric/battery-switch</property>
        </not>
      </condition>
      <volume> 
        <factor>0.7</factor>
      </volume>
    </click>
    
    <!-- ALTERNATORS -->
    <click> 
      <name>Front Alt ON</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonPower.wav</path>
      <condition> 
        <property>controls/electric/engine/generator</property>
      </condition>
      <volume> 
        <factor>0.7</factor>
      </volume>
    </click>
    
    <click> 
      <name>Front Alt OFF</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonPower.wav</path>
      <condition> 
        <not> 
          <property>controls/electric/engine/generator</property>
        </not>
      </condition>
      <volume> 
        <factor>0.7</factor>
      </volume>
    </click>
    
    <!-- FUEL PUMPS -->
    <click> 
      <name>Front Fuel Pump LO ON</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonPower.wav</path>
      <condition> 
        <property>controls/engines/engine/fuel-pumpp</property>
      </condition>
      <volume> 
        <factor>0.7</factor>
      </volume>
    </click>
    
    <click> 
      <name>Front Alt OFF</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonPower.wav</path>
      <condition> 
        <not> 
          <property>controls/engines/engine/fuel-pump</property>
        </not>
      </condition>
      <volume> 
        <factor>0.7</factor>
      </volume>
    </click>
    
    <!-- PITOT HEAT -->
    <click>
      <name>Pitot Heat ON</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <property>controls/anti-ice/engine/carb-heatt</property>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
    
    <click>
      <name>Pitot Heat OFF</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>controls/anti-ice/engine/carb-heat</property>
        </not>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
    
    <!-- NAV LIGHTS -->
    <click>
      <name>Nav Lights ON</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <property>controls/lighting/nav-lights</property>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
    
    <click>
      <name>Nav Lights OFF</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>controls/lighting/nav-lights</property>
        </not>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
    
    <!-- NAV LIGHT -->
    <click>
      <name>Nav Light ON</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <property>controls/lighting/nav-lights</property>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
    
    <click>
      <name>Nav Light OFF</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>controls/lighting/nav-lights</property>
        </not>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
    
    <!-- LANDING LIGHTS -->
    <click>
      <name>Beacon Light ON</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <property>controls/lighting/landing-lights</property>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
    
    <click>
      <name>Beacon Light OFF</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>controls/lighting/landing-lights</property>
        </not>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
       
    <!-- INSTRUMENTS LIGHTS -->
    <click>
      <name>Instruments Light ON</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <property>controls/lighting/instrument-lights</property>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
    
    <click>
      <name>Instruments Light OFF</name>
      <path>Aircraft/Socata-ST10/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>controls/lighting/instrument-lights</property>
        </not>
      </condition>
      <volume>
        <factor>0.6</factor>
      </volume>
    </click>
    
  </fx>
  
</PropertyList>
